{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "peterdee-adf2"
		},
		"ADF2BlobService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADF2BlobService'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLDB'"
		},
		"ADFV2Book_Purchase_Destination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADFV2Book_Purchase_Destination'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "peterdee-adf2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADF2BlobService_connectionString": {
						"value": "[parameters('ADF2BlobService_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSQLDB_connectionString": {
						"value": "[parameters('AzureSQLDB_connectionString')]"
					},
					"ADFV2Book_Purchase_Destination_connectionString": {
						"value": "[parameters('ADFV2Book_Purchase_Destination_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}